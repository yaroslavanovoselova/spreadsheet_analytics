<Window x:Class="Spreadsheet_Analysis.SelectAxisesForPlot"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Spreadsheet_Analysis"
        mc:Ignorable="d"
        Title="Выбор осей" Height="260" Width="500">
    <Grid>
        <Button Content="Готово" HorizontalAlignment="Center" Height="40" Margin="0,173,0,0" VerticalAlignment="Top" Width="140" Click="Button_Click"/>
        <ComboBox x:Name="AxisXComboBox" HorizontalAlignment="Left" Height="40" Margin="30,100,0,0" VerticalAlignment="Top" Width="140"/>
        <ComboBox x:Name="AxisYComboBox" HorizontalAlignment="Left" Height="40" Margin="310,100,0,0" VerticalAlignment="Top" Width="140"/>
        <Label Content="Выберете ось Х:" HorizontalAlignment="Left" Height="35" Margin="30,40,0,0" VerticalAlignment="Top" Width="150"/>
        <Label Content="Выберете ось Y:" HorizontalAlignment="Left" Height="35" Margin="310,40,0,0" VerticalAlignment="Top" Width="150"/>

    </Grid>
</Window>
